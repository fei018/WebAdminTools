@model List<AdminToolsModel.Notes.NotesId>

<hr/>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FirstOrDefault().FileId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FirstOrDefault().FileName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FirstOrDefault().FileDir)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FileId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FileName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FileDir)
        </td>
        <td>
            @Html.ActionLink("Download","Download",new { filename = item.FileName } )
        </td>
    </tr>
}

</table>
